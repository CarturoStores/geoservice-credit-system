{
  "Version": "2008-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": [
          "arn:aws:iam::<id1>:root",
          "arn:aws:iam::<id2>:root",
          "arn:aws:iam::<id3>:root"
        ]
      },
      "Action": ["s3:PutObject", "s3:PutObjectAcl"],
      "Resource": "arn:aws:s3:::my-deployments/*"
    }
  ]
}
